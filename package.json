{
  "name": "bsa-16-okr",
  "version": "0.1.0",
  "description": "BSA-2016 OKR Application",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BinaryStudioAcademy/bsa-2016-okr.git"
  },
  "keywords": [
    "express",
    "webpack",
    "node",
    "bsa16",
    "okr"
  ],
  "authors": [
    "Mykyta Ivanchenko <mivansevdon@gmail.com>",
    "Ihor Oziian <oziian.ihor@gmail.com>",
    "Eugene Dubrovskiy <jknoxville712@gmail.com>"
  ],
  "scripts": {
    "test": "better-npm-run test",
    "test:watch": "better-npm-run test:watch",
    "start": "node server",
    "prod": "better-npm-run prod",
    "prod:c9": "better-npm-run prod:c9",
    "prod:watch": "better-npm-run prod:watch",
    "dev": "better-npm-run dev",
    "dev:watch": "better-npm-run dev:watch",
    "dev:prod": "better-npm-run dev:prod",
    "dev:prod:watch": "better-npm-run dev:prod:watch",
    "seed": "node ./backend/seed/seed.js",
    "seeddrop": "node ./backend/seed/seedDrop.js",
    "archive": "node ./backend/archive/archive.js",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "build:local": "rimraf dist && cross-env NODE_ENV=production LOCAL_PROD=true webpack --config ./webpack.production.local.config.js --progress --profile --colors",
    "eslint": "eslint .",
    "jscs": "jscs ."
  },
  "betterScripts": {
    "prod": {
      "command": "node server",
      "env": {
        "NODE_ENV": "production",
        "PORT": "4444"
      }
    },
    "prod:c9": {
      "command": "npm run build && node server",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "prod:watch": {
      "command": "nodemon server",
      "env": {
        "NODE_ENV": "production",
        "PORT": "4444"
      }
    },
    "dev": {
      "command": "node server",
      "env": {
        "NODE_ENV": "development",
        "WEBPACK": "false",
        "PORT": "4444"
      }
    },
    "dev:watch": {
      "command": "nodemon server",
      "env": {
        "NODE_ENV": "development",
        "WEBPACK": "false",
        "PORT": "4444"
      }
    },
    "dev:prod": {
      "command": "node server",
      "env": {
        "NODE_ENV": "production",
        "LOCAL_PROD": "true",
        "PORT": "4444"
      }
    },
    "dev:prod:watch": {
      "command": "nodemon server",
      "env": {
        "NODE_ENV": "production",
        "LOCAL_PROD": "true",
        "PORT": "4444"
      }
    },
    "test": {
      "command": "node test/index",
      "env": {
        "NODE_ENV": "production",
        "PORT": "4444"
      }
    },
    "test:watch": {
      "command": "nodemon test/index",
      "env": {
        "NODE_ENV": "production",
        "PORT": "4444"
      }
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "alertifyjs": "^1.8.0",
    "async": "^2.0.1",
    "axios": "^0.13.1",
    "babel-cli": "^6.4.0",
    "bluebird": "^3.4.1",
    "chart.js": "^1.1.1",
    "chartjs": "^0.3.24",
    "cookies": "^0.6.1",
    "cron": "^1.1.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.14.1",
    "moment": "^2.14.1",
    "mongoose": "^4.1.10",
    "normalize.css": "^4.2.0",
    "react": "^0.14.8",
    "react-chartjs": "^0.8.0",
    "react-cookie": "^0.4.8",
    "react-date-picker": "^5.3.27",
    "react-dom": "^0.14.8",
    "react-google-charts": "^1.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "request": "^2.74.0",
    "sweetalert": "^1.1.3",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "axios": "^0.13.1",
    "babel-eslint": "^4.1.6",
    "babel-jscs": "^2.0.5",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.3.13",
    "better-npm-run": "0.0.10",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "chai-json-schema": "^1.3.0",
    "chance": "^1.0.4",
    "cross-env": "^1.0.7",
    "css-loader": "^0.19.0",
    "eslint": "^1.5.0",
    "eslint-plugin-react": "^3.4.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "html-webpack-plugin": "^1.6.1",
    "jscs": "^2.1.1",
    "jsdom": "^6.5.1",
    "json-loader": "^0.5.3",
    "mocha": "^2.5.3",
    "mocha-jsdom": "^1.0.0",
    "mongoose": "^4.5.8",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.6.0",
    "react-addons-test-utils": "^0.14.3",
    "react-list": "^0.7.20",
    "react-transform-hmr": "^1.0.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^4.0.0",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  },
  "license": "ISC",
  "bugs": {
