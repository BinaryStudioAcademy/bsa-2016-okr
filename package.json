{
  "name": "bsa-16-okr",
  "version": "0.1.0",
  "description": "BSA-2016 OKR Application",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BinaryStudioAcademy/bsa-2016-okr.git"
  },
  "keywords": [
    "express",
    "webpack",
    "node",
    "bsa16",
    "okr"
  ],
  "author": "BSA-2016",
  "scripts": {
    "test": "better-npm-run test",
    "start": "node server",
    "prod": "better-npm-run prod",
    "prod:watch": "better-npm-run prod:watch",
    "seed": "node ./backend/seed/seed.js",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "eslint": "eslint .",
    "jscs": "jscs ."
  },
  "betterScripts": {
    "prod": {
      "command": "node server",
      "env": {
        "NODE_ENV": "production",
        "PORT": "3000"
      }
    },
    "prod:watch": {
      "command": "nodemon server",
      "env": {
        "NODE_ENV": "production",
        "PORT": "3000"
      }
    },
    "test": {
      "command": "node test/index",
      "env": {
        "NODE_ENV": "production",
        "PORT": "3000"
      }
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "async": "^2.0.1",
    "babel-cli": "^6.4.0",
    "bluebird": "^3.4.1",
    "cookies": "^0.6.1",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.1.8",
    "lodash": "^4.14.1",
    "mongoose": "^4.1.10",
    "normalize.css": "^4.2.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "request": "^2.74.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel-eslint": "^4.1.6",
    "babel-jscs": "^2.0.5",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.3.13",
    "better-npm-run": "0.0.10",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "chance": "^1.0.4",
    "cross-env": "^1.0.7",
    "css-loader": "^0.19.0",
    "eslint": "^1.5.0",
    "eslint-plugin-react": "^3.4.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "html-webpack-plugin": "^1.6.1",
    "jscs": "^2.1.1",
    "jsdom": "^6.5.1",
    "json-loader": "^0.5.3",
    "mocha": "^2.5.3",
    "mocha-jsdom": "^1.0.0",
    "mongoose": "^4.5.8",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.6.0",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-hmr": "^1.0.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^4.0.0",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  },
